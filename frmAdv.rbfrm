	#tag Method, Flags = &h0
		Sub clearAll()
		  cmbAB.DeleteAllRows
		  cmbAC.DeleteAllRows
		  cmbAudCod.DeleteAllRows
		  cmbAF.DeleteAllRows
		  cmbFPS.DeleteAllRows
		  cmbS.DeleteAllRows
		  cmbVB.DeleteAllRows
		  cmbVidCod.DeleteAllRows
		  txtExtra.Text = ""
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub disableAll()
		  cmbAB.Enabled=False
		  cmbAC.Enabled=False
		  cmbAudCod.Enabled=False
		  cmbAF.Enabled=False
		  cmbFPS.Enabled=False
		  cmbS.Enabled=False
		  cmbVB.Enabled=False
		  cmbVidCod.Enabled=False
		  chkS.Value = False
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub toAudio(ext as string)
		  cmbAB.Enabled=True
		  cmbAC.Enabled=True
		  cmbAudCod.Enabled=true
		  cmbAF.Enabled=True
		  
		  cmbFPS.Enabled=False
		  cmbS.Enabled=False
		  cmbVB.Enabled=False
		  cmbVidCod.Enabled=False
		  chkS.Enabled = false
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub toVideo(ext as string)
		  cmbAB.Enabled=True
		  cmbAC.Enabled=true
		  cmbAudCod.Enabled=true
		  cmbAF.Enabled=true
		  cmbFPS.Enabled=True
		  chkS.Enabled = true
		  
		  if (ext="3gp") or (ext="mp4") then
		    chkS.Value=True
		  else
		    chkS.Value=false
		  end if
		  
		  cmbVB.Enabled=True
		  cmbVidCod.Enabled=true
		End Sub
	#tag EndMethod


