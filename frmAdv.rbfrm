#tag Events chkS
	#tag Event
		Sub Action()
		  
		  
		End Sub
	#tag EndEvent
	#tag Event
		Function MouseDown(X As Integer, Y As Integer) As Boolean
		  
		  
		  select case chkS.Value
		    
		  case false
		    
		    dim iVFpos as Integer
		    iVFpos = InStr( txtExtra.Text, " -vf ")
		    if (iVFpos = 0) then
		      
		      dim i as Integer
		      i = instr(0, txtExtra.Text, "$INPUT")
		      if (i <> 0) then
		        txtExtra.Text = left(txtExtra.Text, i+len("$INPUT")) + " -vf scale=$VS " + right(txtExtra.Text, len(txtExtra.Text) - i - len("$INPUT") )
		      end if
		      
		    else
		      'MsgBox  left(cmd, iVFpos+len(" -vf"))
		      'MsgBox right(cmd, len(cmd) - iVFpos - len(" -vf") )
		      txtExtra.Text = left(txtExtra.Text, iVFpos+len(" -vf")) + "scale=$VS," + right(txtExtra.Text, len(txtExtra.Text) - iVFpos - len(" -vf") )
		    end if
		    
		    
		  case true
		    
		    
		    if (InStr(txtExtra.Text, "-vf scale=$VS,") <> 0) then
		      txtExtra.Text = ReplaceAllB(txtExtra.Text, "scale=$VS,", "")
		    elseif (InStr(txtExtra.Text, ",scale=$VS") <> 0) then
		      txtExtra.Text = ReplaceAllB(txtExtra.Text, "scale=$VS,", "")
		    elseif (InStr(txtExtra.Text, "-vf scale=$VS") <> 0) then
		      txtExtra.Text = ReplaceAllB(txtExtra.Text, "-vf scale=$VS", " ")
		    end if
		    
		    
		  end Select
		  
		  cmbS.Enabled = Not chkS.Value
		End Function
	#tag EndEvent
#tag EndEvents
